{
    "name": "@ngfancy/ng-bs-switch",
    "version": "1.0.3",
    "description": "A switch for angular with bootstrap",
    "main": "dist/ng-bs-switch.js",
    "scripts": {
        "rimraf": "rimraf",
        "typings": "typings",
        "webpack": "webpack",
        "webpack-dev-server": "webpack-dev-server",
        "sass": "node-sass",
        "clean": "npm cache clean && npm run rimraf -- node_modules doc typings coverage dist",
        "clean:dist": "npm run rimraf -- dist",
        "preclean:install": "npm run clean",
        "clean:install": "npm set progress=false && npm install",
        "preclean:start": "npm run clean",
        "clean:start": "npm start",
        "prebuild": "npm run clean:dist",
        "build": "npm run build:dist && npm run sass dist/ng-bs-switch.scss dist/ng-bs-switch.css",
        "build:dist": "webpack --config webpack.build.js --progress --profile --colors --display-error-details --display-cached --bail",
        "postbuild": "mv dist/src/ng-bs-switch.d.ts dist/ng-bs-switch.d.ts && rm -rf dist/src",
        "server": "npm run server:dev",
        "server:dev": "webpack-dev-server --config webpack.dev.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
        "server:dev:hmr": "npm run server:dev -- --hot",
        "start": "npm run server:dev",
        "test": "karma start"
    },
    "author": "Bob",
    "license": "MIT",
    "dependencies": {},
    "devDependencies": {
        "@types/angular": "^1.5.13",
        "@types/angular-mocks": "^1.5.4",
        "@types/mocha": "^2.2.31",
        "@types/node": "^6.0.38",
        "@types/webpack": "^1.12.33",
        "angular": "^1.5.8",
        "angular-mocks": "^1.5.8",
        "awesome-typescript-loader": "^2.2.1",
        "bootstrap-sass": "^3.3.7",
        "chai": "^3.5.0",
        "copy-webpack-plugin": "^2.1.3",
        "css-loader": "^0.23.1",
        "file-loader": "^0.8.5",
        "html-webpack-plugin": "^2.16.0",
        "istanbul-instrumenter-loader": "^0.2.0",
        "json-loader": "^0.5.4",
        "karma": "^1.2.0",
        "karma-chrome-launcher": "^2.0.0",
        "karma-coverage": "^1.1.1",
        "karma-mocha": "^1.1.1",
        "karma-mocha-reporter": "^2.1.0",
        "karma-phantomjs-launcher": "^1.0.2",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "^1.8.0",
        "mocha": "^3.0.2",
        "node-sass": "^3.6.0",
        "raw-loader": "^0.5.1",
        "remap-istanbul": "^0.6.4",
        "rimraf": "^2.5.2",
        "sass-loader": "^3.2.0",
        "sinon": "^1.17.5",
        "sinon-chai": "^2.8.0",
        "source-map-loader": "^0.1.5",
        "style-loader": "^0.13.1",
        "typescript": "^2.0.1",
        "url-loader": "^0.5.7",
        "webpack": "^1.13.0",
        "webpack-dev-server": "^1.14.1"
    }
}